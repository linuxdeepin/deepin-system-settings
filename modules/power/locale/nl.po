#: ../src/tray_shutdown_plugin.py:53
#, python-format
msgid ""
"\n"
"<span foreground='#cc5744'>Turn off</span> your computer now? \n"
"\n"
"The system will shut down in %s seconds."
msgstr ""
"\n"
"Nu uw computer <span foreground='#cc5744'>Afsluiten</span>? \n"
"\n"
"Het systeem zal afsluiten over %s seconden."

#: ../src/tray_shutdown_plugin.py:56
#, python-format
msgid ""
"\n"
"<span foreground='#cc5744'>Restart</span> your computer now?\n"
"\n"
"The system will restart in %s seconds."
msgstr ""
"\n"
"Nu uw computer <span foreground='#cc5744'>Herstarten</span>?\n"
"\n"
"Het systeem zal herstarten over %s seconden."

#: ../src/tray_shutdown_plugin.py:59
#, python-format
msgid ""
"\n"
"<span foreground='#cc5744'>Suspend</span> your computer now?\n"
"\n"
"The system will suspend in %s seconds."
msgstr ""
"\n"
"Nu uw computer zetten in <span foreground='#cc5744'>Slaapstand</span>?\n"
"\n"
"Het systeem zal sluimeren over %s seconden."

#: ../src/tray_shutdown_plugin.py:62
#, python-format
msgid ""
"\n"
"<span foreground='#cc5744'>Log out</span> of your system now?\n"
"\n"
"You will be automatically logged out in %s seconds."
msgstr ""
"\n"
"Nu uw computer <span foreground='#cc5744'>Uitloggen</span>?\n"
"\n"
"U zal automatisch uitgelogd worden over %s seconden."

