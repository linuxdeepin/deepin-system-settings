#: ../src/tray_shutdown_plugin.py:53
#, python-format
msgid ""
"\n"
"<span foreground='#cc5744'>Turn off</span> your computer now? \n"
"\n"
"The system will shut down in %s seconds."
msgstr ""
"\n"
"<span foreground='#cc5744'>Matikan</span> komputer Anda sekarang?\n"
"Sistem akan menutup dalam %s detik."

#: ../src/tray_shutdown_plugin.py:56
#, python-format
msgid ""
"\n"
"<span foreground='#cc5744'>Restart</span> your computer now?\n"
"\n"
"The system will restart in %s seconds."
msgstr ""
"\n"
"<span foreground='#cc5744'>Restart</span> komputer Anda sekarang?\n"
"Sistem akan melakukan restart dalam %s detik."

#: ../src/tray_shutdown_plugin.py:59
#, python-format
msgid ""
"\n"
"<span foreground='#cc5744'>Suspend</span> your computer now?\n"
"\n"
"The system will suspend in %s seconds."
msgstr ""
"\n"
"<span foreground='#cc5744'>Suspend</span> komputer Anda sekarang?\n"
"Sistem akan melakukan suspend dalam %s detik."

#: ../src/tray_shutdown_plugin.py:62
#, python-format
msgid ""
"\n"
"<span foreground='#cc5744'>Log out</span> of your system now?\n"
"\n"
"You will be automatically logged out in %s seconds."
msgstr ""
"\n"
"<span foreground='#cc5744'>Log out</span> dari sistem Anda sekarang?\n"
"Anda akan log out otomatis dalam %s detik."

#: ../src/tray_shutdown_plugin.py:65
msgid ""
"\n"
"<span foreground='#cc5744'>A program is still running:</span>"
msgstr ""
"\n"
"<span foreground='#cc5744'>Masih ada program yang berjalan:</span>"

#: ../src/tray_shutdown_plugin.py:66
#, python-format
msgid ""
